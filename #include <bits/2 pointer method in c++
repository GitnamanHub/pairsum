#include <bits/stdc++.h> 
int pairSum(vector<int> &arr, int n, int target){
	int count{0};
 int i=0,j=n-1;
	while(i<j)
	{
		if(arr.at(i)+arr.at(j)==target)
		{
			count++;
			i++;
			j--;
		}

		else if(arr.at(i)+arr.at(j)>target)
		{
			j--;
		}

		else if(arr.at(i)+arr.at(j)<target)
		{
			i++;
		}
	}

	if(count==0)
    return count-1;
	else
	return count;
}
